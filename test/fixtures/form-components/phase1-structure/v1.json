          "children": [
            {
              "id": "name-label",
              "type": "text",
              "content": "Full Name",
              "size": "sm",
              "weight": "medium",
              "color": "#525252"
            },
            {
              "id": "name-input",
              "type": "input",
              "placeholder": "Enter your full name",
              "layout": {
                "background": "#E5E5E5",
                "border": "1px solid #E5E5E5",
                "border_radius": 6,
                "padding": 12
              }
            }
          ]
        },
        {
          "id": "email-field",
          "type": "box",
          "layout": {
            "display": "flex",
            "direction": "vertical",
            "gap": 8
          },
          "children": [
            {
              "id": "email-label",
              "type": "text",
              "content": "Email Address",
              "size": "sm",
              "weight": "medium",
              "color": "#525252"
            },
            {
              "id": "email-input",
              "type": "input",
              "placeholder": "you@example.com",
              "layout": {
                "background": "#E5E5E5",
                "border": "1px solid #E5E5E5",
                "border_radius": 6,
                "padding": 12
