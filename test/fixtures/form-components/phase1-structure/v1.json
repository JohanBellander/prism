{
  "version": "v1",
  "phase": "structure",
  "created_at": "2025-10-25T12:30:00Z",
  "locked": false,
  "intent": {
    "purpose": "Registration form with multiple input fields and validation",
    "primary_action": "Create account",
    "user_context": "New users signing up for the service",
    "key_interactions": ["fill_form", "submit", "social_login"]
