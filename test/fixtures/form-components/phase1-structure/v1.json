      "children": [
        {
          "id": "form-title",
          "type": "text",
          "content": "Create Account",
          "size": "3xl",
          "weight": "bold",
          "color": "#000000"
        },
        {
          "id": "form-subtitle",
          "type": "text",
          "content": "Fill in your details to get started",
          "size": "sm",
          "color": "#737373"
        },
        {
          "id": "name-field",
          "type": "box",
          "layout": {
            "display": "flex",
            "direction": "vertical",
            "gap": 8
